<%= form_with model: @kitten do |form| %>
  <div class="input-kitten">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div class="input-kitten">
    <%= form.label :age %>
    <%= form.number_field :age %>
  </div>
  <div class="input-kitten">
    <%= form.label :cuteness %>
    <%= form.text_field :cuteness%>
  </div>
  <div class="input-kitten">
    <%= form.label :softness %>
    <%= form.text_field :softness %>
  </div>

  <%= form.submit %>
<% end %>
  